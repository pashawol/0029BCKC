mixin sPartners(data)
	// start sPartners
	+b.SECTION.sPartners--js.section#sPartners&attributes(attributes)
		.container
			+b.section-title 
						h2 Партнеры 
			+e.slider-wrap
				.swiper.sPartners-slider-js

					.swiper-wrapper
						+sPartnersItem(1, 'МЧС России').bigger-col
						+sPartnersItem(2, 'Минобрнауки').bigger-col
						+sPartnersItem(3, 'Министерство просвещения').bigger-col
						+sPartnersItem(4, 'Росмолодежь').bigger-col
						+sPartnersItem(4)
						+sPartnersItem(3)
						-for(let i = 0; i < 2; i++)
							+sPartnersItem(5)
							+sPartnersItem(6)
							+sPartnersItem(7)
							+sPartnersItem(8)
							+sPartnersItem(9)
							+sPartnersItem(10)
					.swiper-pagination
				+e.card-row-prev.swiper-button-hand.swiper-prev
					svg.icon.icon-chevron-left
						use(xlink:href='img/svg/sprite.svg#chevron-left')
				+e.card-row-next.swiper-button-hand.swiper-next
					svg.icon.icon-chevron-right
						use(xlink:href='img/svg/sprite.svg#chevron-right')
	// end sPartners

mixin sPartnersItem(imgNum, text='Партнер')
	.swiper-slide
		+e.item
			+e.img(href="#")
				img(loading="lazy" src=`img/@2x/sPartners-${imgNum}.png` alt="")
			//- p!= text

